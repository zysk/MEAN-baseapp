{
  "name": "MEAN-baseapp",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "14.*",
    "npm": "6.*"
  },
  "lint-staged": {
    "views/development/MEAN-baseapp/src/**/*.{js,jsx,ts,tsx,json,css,scss}": [
      "npm run format",
      "npm run lint:fix"
    ]
  },
  "scripts": {
    "postinstall": "husky install",
    "pre-commit": "lint-staged",
    "typesync": "typesync --ignoredeps=dev",
    "format": "prettier \"views/development/angular-baseapp/src/**/*.{js,jsx,ts,tsx,json,css,scss}\" --write",
    "lint": "eslint \"views/development/angular-baseapp/src/**/*.{js,jsx,ts,tsx,json,css,scss}\" --fix",
    "lint:fix": "eslint \"views/development/angular-baseapp/src/**/*.{js,jsx,ts,tsx,json,css,scss}\" --fix --quiet --fix-type layout --format=codeframe",
    "lint:check": "eslint \"views/development/angular-baseapp/src/**/*.{js,jsx,ts,tsx,json,css,scss}\" --fix-dry-run --fix-type layout --format=codeframe",
    "start:dev": "nodemon ./bin/www && set DEBUG=MEAN-baseapp:*",
    "start:prod": "node --optimize-for-size --gc-interval=2 ./bin/www"
  },
  "dependencies": {
    "compression": "1.7.4",
    "cookie-parser": "1.4.6",
    "debug": "4.3.4",
    "dotenv": "16.0.1",
    "express": "4.18.1",
    "express-session": "1.17.3",
    "http-errors": "2.0.0",
    "morgan": "1.10.0",
    "nodemon": "2.0.19",
    "request": "2.88.2",
    "serve-favicon": "2.5.0",
    "swig-templates": "2.0.3"
  },
  "devDependencies": {
    "@types/compression": "1.7.2",
    "@types/cookie-parser": "1.4.3",
    "@types/debug": "4.1.7",
    "@types/express": "4.17.13",
    "@types/express-session": "1.17.3",
    "@types/http-errors": "1.8.2",
    "@types/morgan": "1.9.3",
    "@types/nodemon": "1.19.2",
    "@types/request": "2.48.8",
    "@types/serve-favicon": "2.5.0",
    "@typescript-eslint/eslint-plugin": "5.16.0",
    "@typescript-eslint/parser": "5.16.0",
    "eslint": "8.17.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "16.2.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-formatter-codeframe": "7.32.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prefer-object-spread": "1.2.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-security": "1.5.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "prettier": "2.6.0",
    "typescript": "4.6.2",
    "typesync": "0.9.0"
  }
}
